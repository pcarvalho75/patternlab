<UserControl x:Class="BelaGraph.BelaGraphControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid MinWidth="200" MinHeight="200" Background="White">
        <Image Height="80" Name="imageBelaGraphLogo" Margin="0,20,20,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="80" Opacity="0.85">
        </Image>
        <Canvas Margin="90,40,30,50" Name="TheGraph"/>
        <Label Name="labelTitle" VerticalAlignment="Top" HorizontalContentAlignment="Center" FontSize="20" Height="34.14"></Label>
        <Canvas Height="30" Margin="90,0,30,20" Name="canvasX" VerticalAlignment="Bottom" />
        <Canvas HorizontalAlignment="Left" Margin="0,40,0,50" Name="canvasY" Width="90"></Canvas>
        <Label Height="25" Margin="90,0,30,0" Name="labelXLabel" VerticalAlignment="Bottom" HorizontalAlignment="Center"></Label>
        <Label Margin="0,0,0,0" Name="labelyLabel" RenderTransformOrigin="0.5,0.5" Height="26" VerticalAlignment="Center" HorizontalAlignment="Left"></Label>
    </Grid>
</UserControl>
